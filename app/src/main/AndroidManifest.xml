<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.statisticator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.requests.ResultCortegesActivity"
            android:label="@string/corteges_list_title"></activity>
        <activity android:name=".ui.requests.ResultGroupsActivity"></activity>
        <activity
            android:name=".ui.EventEditingActivity"
            android:label="@string/event_editing_title"></activity>
        <activity
            android:name=".ui.requests.RequestsListActivity"
            android:label="@string/requests_list_title"></activity>
        <activity
            android:name=".ui.requests.RequestEditingActivity"
            android:label="@string/request_editing_title">
        </activity>
        <activity
            android:name=".ui.MenuActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>